{
  "name": "vnftjs",
  "version": "1.2.2",
  "description": "Discord CommandHandler for TypeScript or JavaScript",
  "main": "lib",
  "types": "lib",
  "scripts": {
    "test": "rm -rf lib && tsc && node ./lib/secret.test.js",
    "prettier": "prettier --print-width 120 --write ./src/{*,*/*,*/*/*,*/*/*/*}.ts",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "build": "rm -rf lib && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VonFriedricht/vnftjs.git"
  },
  "keywords": [
    "discord",
    "bot",
    "node",
    "discordapp",
    "command",
    "commandhandler"
  ],
  "author": "VonFriedricht",
  "license": "ISC",
  "runkitExampleFilename": "./docs/examples/runkit.ts",
  "dependencies": {
    "discord.js": "^11.5.0",
    "vnft-tools": "^1.0.9-b"
  },
  "devDependencies": {
    "jsdoc": "^3.6.2",
    "minami": "^1.2.3"
  }
}
